<Page x:Class="Squire.Views.Pages.Infos"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Squire.Views.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Infos" 
      FontFamily="/Fonts/#cabin" Foreground="White">

   <Grid>
      <StackPanel Name="MotherPanel" Margin="40">
         <Image Source="/Assets/Logo/CircledWhite_logo.png" Opacity=".1"
                Height="{Binding ElementName=MotherPanel, Path=ActualHeight}"/>
      </StackPanel>
   </Grid>
</Page>
